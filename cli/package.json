{
  "name": "matano",
  "version": "0.0.0",
  "description": "Matano - the open source security lake platform for AWS.",
  "author": "ðŸ»â€â„ï¸ Samrose Ahmed ðŸ»â€â„ï¸",
  "bin": {
    "matano": "./bin/run"
  },
  "homepage": "https://github.com/matanolabs/matano",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.9.5",
    "@oclif/plugin-autocomplete": "*",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "enquirer": "^2.3",
    "execa": "^5",
    "ora": "^4",
    "chalk": "^4"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/node": "^16.9.4",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.7"
  },
  "oclif": {
    "bin": "matano",
    "dirname": "matano",
    "helpClass": "./dist/help",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ],
    "topicSeparator": ":",
    "topics": {
      "generate": {
        "description": "Utilities to get started and generate boilerplate."
      }
    }
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules",
    "full-install": "npm ci && npm run build && npm uninstall -g matano && npm install -g .",
    "build": "rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/matanolabs/matano/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
